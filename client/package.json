{
  "name": "relayable",
  "version": "1.0.0",
  "description": "Simple relay proxy that intercepts and forwards requests from the internet towards connected clients",
  "main": "start.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
