    "nodemon": "^2.0.7",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "dotenv": "^14.2.0",
    "socket.io-client": "^4.4.1"
  },
